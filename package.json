{
  "name": "solid-start-sitemap",
  "version": "0.1.0",
  "description": "Solid-start Sitemap generator",
  "license": "MIT",
  "author": "madaxen86",
  "contributors": [],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/madaxen86/solid-start-sitemap.git"
  },
  "homepage": "https://github.com/madaxen86/solid-start-sitemap#readme",
  "bugs": {
    "url": "https://github.com/madaxen86/solid-start-sitemap/issues"
  },
  "files": [
    "dist"
  ],
  "private": false,
  "sideEffects": false,
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "browser": {},
  "exports": {
    "import": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    }
  },
  "typesVersions": {},
  "scripts": {
    "dev": "vite serve dev",
    "build": "tsup",
    "test": "concurrently pnpm:test:*",
    "test:client": "vitest",
    "test:ssr": "pnpm run test:client --mode ssr",
    "prepublishOnly": "pnpm build",
    "format": "prettier --ignore-path .gitignore -w \"src/**/*.{js,ts,json,css,tsx,jsx}\" \"dev/**/*.{js,ts,json,css,tsx,jsx}\"",
    "lint": "concurrently pnpm:lint:*",
    "lint:code": "eslint --ignore-path .gitignore --max-warnings 0 ssrc/**/*.{js,ts,tsx,jsx}",
    "lint:types": "tsc --noEmit",
    "update-deps": "pnpm up -Li"
  },
  "peerDependencies": {
    "solid-js": "^1.6.0",
    "vinxi": "^0.4.3"
  },
  "dependencies": {
    "sitemap": "^8.0.0"
  },
  "devDependencies": {
    "@changesets/cli": "^2.27.9",
    "@types/node": "^22.7.5",
    "@typescript-eslint/eslint-plugin": "^8.8.1",
    "@typescript-eslint/parser": "^8.8.1",
    "concurrently": "^9.0.1",
    "esbuild": "^0.24.0",
    "eslint": "^9.12.0",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-no-only-tests": "^3.3.0",
    "jsdom": "^25.0.1",
    "prettier": "3.3.3",
    "solid-js": "^1.9.2",
    "tsup": "^8.3.0",
    "typescript": "^5.6.3",
    "vinxi": "^0.4.3",
    "vite": "^5.4.8",
    "vitest": "^2.1.2"
  },
  "keywords": [
    "solid"
  ],
  "packageManager": "pnpm@9.1.1",
  "engines": {
    "node": ">=18",
    "pnpm": ">=9.0.0"
  }
}
