{
  "name": "solid-start-typesafe-routes-plugin",
  "version": "1.2.0",
  "description": "Type-safe routes for solid-start file-routes. No more broken links.",
  "license": "MIT",
  "author": "madaxen86",
  "contributors": [],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/madaxen86/solid-start-typesafe-fileroutes.git"
  },
  "homepage": "https://github.com/madaxen86/solid-start-typesafe-fileroutes",
  "bugs": {
    "url": "https://github.com/madaxen86/solid-start-typesafe-fileroutes/issues"
  },
  "files": [
    "dist"
  ],
  "private": false,
  "sideEffects": false,
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "browser": {},
  "exports": {
    "import": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    }
  },
  "typesVersions": {},
  "scripts": {
    "dev": "vite serve dev",
    "build": "tsup",
    "test": "vitest",
    "prepublishOnly": "pnpm build",
    "format": "prettier --ignore-path .gitignore -w \"src/**/*.{js,ts,json,css,tsx,jsx}\" \"dev/**/*.{js,ts,json,css,tsx,jsx}\"",
    "lint": "concurrently pnpm:lint:*",
    "lint:code": "eslint --ignore-path .gitignore --max-warnings 0 ssrc/**/*.{js,ts,tsx,jsx}",
    "lint:types": "tsc --noEmit",
    "update-deps": "pnpm up -Li",
    "publish": "pnpm changeset version && pnpm publish"
  },
  "peerDependencies": {
    "solid-js": ">=1.6.0"
  },
  "devDependencies": {
    "@changesets/cli": "^2.28.1",
    "@types/node": "^22.13.4",
    "@typescript-eslint/eslint-plugin": "^8.24.1",
    "@typescript-eslint/parser": "^8.24.1",
    "concurrently": "^9.1.2",
    "esbuild": "^0.24.2",
    "eslint": "^9.20.1",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-no-only-tests": "^3.3.0",
    "jsdom": "^25.0.1",
    "prettier": "3.3.3",
    "solid-js": "^1.9.4",
    "tsup": "^8.3.6",
    "typescript": "^5.7.3",
    "vite": "^6.1.1",
    "vitest": "^3.0.6"
  },
  "keywords": [
    "solid"
  ],
  "packageManager": "pnpm@9.1.1",
  "engines": {
    "node": ">=18",
    "pnpm": ">=9.0.0"
  }
}
